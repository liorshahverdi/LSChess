package chess;

public class Move {
	private BoardCell fromCell;
	private BoardCell toCell;
	private Piece movedPiece;
	
	public Move(BoardCell f, BoardCell t, Piece p){
		this.fromCell = f;
		this.toCell = t;
		this.movedPiece = p;
	}
	
	public String toString(){ return "From="+this.fromCell.toString()+"\tTo="+this.toCell.toString(); }
}
